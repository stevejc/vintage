<div class="row">
  <div class="span6">
  	<h3 class="red" rel="popover">Orders</h3>
  </div>

</div>

<table class="table table-hover">
  <tr>
    <th>Status</th>
    <th>Order Date</th>
  </tr>

<% @orders.each do |order| %>
  <% if status == "Store" %>
    <tr class="popout" rel="popover" data-trigger="hover" data-content= "<%= display_items_in_order(order) %>" data-original-title="Items in order:" data-placement="bottom" data-link="<%= yourshoporder_path(order) %>">
  <% else %>
    <tr data-link="<%= yourorder_path(order) %>">
  <% end %>
    <td><%= order.status %></td>
    <td><%= order.created_at %></td>
    
  </tr>
<% end %>
</table>